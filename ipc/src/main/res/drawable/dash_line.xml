<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#ffffff"/>
            <stroke android:color="#000000" android:width="1dp" android:dashWidth="10dp"
                    android:dashGap="5dp"/>
